{
  "name": "vite-react-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "create-ui-component": "node ./scripts/logic/create-ui-component.cjs",
    "remove-console-logs": "node ./scripts/logic/comment-console-logs.cjs",
    "set-env": "node ./scripts/logic/write-env.cjs",
    "init:firebase:scripts": "node ./scripts/logic/initFirebaseScripts.cjs",
    "repath": "node ./scripts/logic/newPaths.cjs",
    "set:multilang": "node --no-warnings=ExperimentalWarning ./scripts/logic/setMultiLang.cjs",
    "set:firebase": "firebase init && npm run init:firebase:scripts",
    "add:firebase": "firebase use --add && npm run init:firebase:scripts"
  },
  "dependencies": {
    "million": "^3.1.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-phone-number-input": "^3.4.11",
    "react-router-dom": "^7.1.3",
    "sass": "^1.83.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@inquirer/prompts": "^7.2.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.14.0",
    "inquirer-select-pro": "^1.0.0-alpha.9",
    "prettier": "^3.5.3",
    "vite": "^6.0.5"
  }
}
